{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5200",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionStrings__Default": "Host=localhost;Port=5432;Database=pointr_case;Username=postgres;Password=postgres"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "http://localhost:8080/swagger",
      "publishAllPorts": false,
      "useSSL": false,
      "environmentVariables": {
        "ASPNETCORE_HTTP_PORTS": "8080",
        "ConnectionStrings__Default": "Host=host.docker.internal;Port=5432;Database=pointr_case;Username=postgres;Password=postgres"
      }
    }
  }
}